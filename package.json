{
  "name": "simple-node-server",
  "version": "1.0.0",
  "description": "markdown server using https module",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest",
    "dev": "ts-node src/index.ts",
    "build:local": "tsc",
    "build": "npm run build:local",
    "clean:build": "rm -rf lib",
    "docker:image-build": "docker build . -t couch-gag:latest -t couch-gag-server:latest",
    "docker:container-run": "docker run -d -p 2023:2023 --name couch-gag couch-gag:latest",
    "compose:up": "docker-compose -f docker-compose.yml up -d --build",
    "compose:down": "docker-compose down",
    "start": "node lib/index.js",
    "fmt": "prettier --write ."
  },
  "author": "@nickgdev",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/firestore": "^6.0.0",
    "@nickgdev/couch-gag-common-lib": "^1.1.62",
    "@vercel/node": "^1.15.1",
    "chalk": "4.1.2",
    "express": "^4.18.1",
    "gray-matter": "^4.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.25",
    "@types/uuid": "^8.3.4",
    "dotenv": "^16.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.3"
  }
}
